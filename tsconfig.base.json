{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@happynrwl/shared-ui": ["libs/shared-ui/src/index.ts"],
      "@happynrwl/store/ui-shared": ["libs/store/ui-shared/src/index.ts"],
      "@happynrwl/ui/ui-footer": ["libs/ui/ui-footer/src/index.ts"],
      "@happynrwl/ui/ui-header": ["libs/ui/ui-header/src/index.ts"],
      "@happynrwl/ui/ui-typography": ["libs/ui/ui-typography/src/index.ts"],
      "@happynrwl/utils/utils-stringify": [
        "libs/utils/utils-stringify/src/index.ts"
      ]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
